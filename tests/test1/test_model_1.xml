<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE xmodel SYSTEM "http://eurace.cs.bilgi.edu.tr/XMML.dtd">
<xmodel version="2">

<name>test_model_simple_everthing</name>
<version>01</version>
<description></description>

<models>
  <model><file>test_model_1_sub_1.xml</file><enabled>true</enabled></model>
  <model><file>test_model_1_sub_2.xml</file><enabled>true</enabled></model>
</models>

<environment>

<constants>
  <variable>
  <type>int</type><name>my_constant</name>
  <description>value read in from 0.xml</description>
  </variable>
</constants>

<functionFiles>
  <file>agent_a_functions.c</file>
  <file>agent_b_functions.c</file>
</functionFiles>

<timeUnits>
<timeUnit>
 <name>daily</name>
 <unit>iteration</unit>
 <period>1</period>
</timeUnit>
<timeUnit>
 <name>weekly</name>
 <unit>daily</unit>
 <period>5</period>
</timeUnit>
<timeUnit>
 <name>monthly</name>
 <unit>weekly</unit>
 <period>4</period>
</timeUnit>
</timeUnits>

<dataTypes>
<dataType>
<name>my_datatype_1</name>
<description></description>
<variables>
  <variable><type>int</type><name>id</name><description></description></variable>
  <variable><type>int</type><name>type</name><description></description></variable>
</variables>
</dataType>
<dataType>
<name>my_datatype_2</name>
<description></description>
<variables>
  <variable><type>my_datatype_1</type><name>var_my_datatype_1</name><description></description></variable>
</variables>
</dataType>
</dataTypes>

</environment>

<agents>

<xagent>
<name>agent_a</name>
<description></description>
<memory>
  <variable><type>int</type><name>id</name><description></description></variable>
  <variable><type>int</type><name>my_int_array[2]</name><description></description></variable>
  <variable><type>int_array</type><name>my_dyn_int_array</name><description></description></variable>
  <variable><type>my_datatype_1</type><name>var_my_datatype_1</name><description></description></variable>
  <variable><type>my_datatype_2</type><name>var_my_datatype_2</name><description></description></variable>
</memory>
</xagent>

<xagent>
<name>agent_b</name>
<description></description>
<memory>
<variable><type>int</type><name>id</name><description></description></variable>
<variable><type>int</type><name>type</name><description></description></variable>
</memory>
</xagent>

</agents>

</xmodel>
